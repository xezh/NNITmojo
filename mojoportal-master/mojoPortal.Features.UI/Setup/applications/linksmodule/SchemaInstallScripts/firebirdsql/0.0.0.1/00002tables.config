CREATE TABLE MP_LINKS
(
  ITEMID Integer NOT NULL,
  MODULEID Integer NOT NULL,
  TITLE Varchar(255),
  URL Varchar(255),
  TARGET Varchar(20) NOT NULL,
  VIEWORDER Integer,
  DESCRIPTION Blob sub_type 1,
  CREATEDDATE Timestamp,
  CREATEDBY Integer,
  ITEMGUID Varchar(36),
  MODULEGUID Varchar(36),
  USERGUID Varchar(36),
  PRIMARY KEY (ITEMID)
);
