SET TERM ^ ;
ALTER PROCEDURE MP_RSSFEEDS_INSERT (
    MODULEID Integer,
    CREATEDDATE Timestamp,
    USERID Integer,
    AUTHOR Varchar(100),
    URL Varchar(255),
    RSSURL Varchar(255),
    ITEMGUID Char(36),
    MODULEGUID Char(36),
    USERGUID Char(36),
    IMAGEURL Varchar(255),
    FEEDTYPE Varchar(20),
    PUBLISHBYDEFAULT Integer )
RETURNS (
    ITEMID Integer )
AS
BEGIN
 ITEMID = NEXT VALUE FOR mp_RssFeeds_seq;

INSERT INTO 	MP_RSSFEEDS
(				
                ITEMID,
                MODULEID,
                CREATEDDATE,
                USERID,
                AUTHOR,
                URL,
                RSSURL,
		ITEMGUID,
		MODULEGUID,
		USERGUID,
		LASTMODUSERGUID,
                LASTMODUTC,
        IMAGEURL,
                FEEDTYPE,
                PUBLISHBYDEFAULT
) 
VALUES 
(				
               :ITEMID,
               :MODULEID,
               :CREATEDDATE,
               :USERID,
               :AUTHOR,
               :URL,
               :RSSURL,
	      :ITEMGUID,
	      :MODULEGUID,
	      :USERGUID,
	      :USERGUID,
               :CREATEDDATE,
    :IMAGEURL,
                :FEEDTYPE,
                :PUBLISHBYDEFAULT
);

END^
SET TERM ; ^
