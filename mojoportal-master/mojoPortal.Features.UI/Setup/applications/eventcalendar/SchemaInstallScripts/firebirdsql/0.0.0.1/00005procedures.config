SET TERM ^ ;
ALTER PROCEDURE MP_CALENDAREVENTS_INSERT (
    MODULEID Integer,
    TITLE Varchar(255),
    DESCRIPTION Blob sub_type 1,
    IMAGENAME Varchar(100),
    EVENTDATE Timestamp,
    STARTTIME Time,
    ENDTIME Time,
    CREATEDDATE Timestamp,
    USERID Integer,
    ITEMGUID Char(36),
    MODULEGUID Char(36),
    USERGUID Char(36),
    LOCATION Blob sub_type 1,
    TICKETPRICE Decimal(15,4),
    REQUIRESTICKET Smallint )
RETURNS (
    ITEMID Integer )
AS
BEGIN
 ITEMID = NEXT VALUE FOR mp_CalendarEvents_seq;

INSERT INTO 	MP_CALENDAREVENTS
(				
                ITEMID,
                MODULEID,
                TITLE,
                DESCRIPTION,
                IMAGENAME,
                EVENTDATE,
                STARTTIME,
                ENDTIME,
                CREATEDDATE,
                USERID,
		ITEMGUID,
		MODULEGUID,
		USERGUID,
		LOCATION,
		LASTMODUSERGUID,
		LASTMODUTC,
		TICKETPRICE,
                REQUIRESTICKET
) 
VALUES 
(				
               :ITEMID,
               :MODULEID,
               :TITLE,
               :DESCRIPTION,
               :IMAGENAME,
               :EVENTDATE,
               :STARTTIME,
               :ENDTIME,
               :CREATEDDATE,
               :USERID,
	      :ITEMGUID,
	      :MODULEGUID,
	      :USERGUID,
	      :LOCATION,
	      :USERGUID,
	      :CREATEDDATE,
	      :TICKETPRICE,
               :REQUIRESTICKET
);

END^
SET TERM ; ^