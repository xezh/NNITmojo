CREATE TABLE "mp_contenttemplate" (	"guid" char(36) NOT NULL,     "siteguid" char(36) NOT NULL,    "title" varchar(255) NOT NULL,    "imagefilename" varchar(255) NULL,    "description" text NULL,    "body" text NOT NULL,    "allowedroles" text NULL,    "createdbyuser" char(36) NOT NULL,    "lastmoduser" char(36) NOT NULL,    "createdutc" timestamp NOT NULL,    "lastmodutc" timestamp NOT NULL	);	CREATE UNIQUE INDEX "mp_contenttemplate_pkey"  ON "mp_contenttemplate"("guid");     ALTER TABLE mp_contenttemplateADD CONSTRAINT pk_contenttemplate PRIMARY KEY (guid); CREATE INDEX idxcttitle ON mp_contenttemplate(title);CREATE TABLE "mp_contentstyle" (	"guid" char(36) NOT NULL,     "siteguid" char(36) NOT NULL,    "name" varchar(100) NOT NULL,    "element" varchar(50) NOT NULL,    "cssclass" varchar(50) NOT NULL,    "skinname" varchar(100) NOT NULL,    "isactive" bool NOT NULL,    "createdutc" timestamp NOT NULL,    "lastmodutc" timestamp NOT NULL,    "createdby" char(36) NOT NULL,    "lastmodby" char(36) NOT NULL	);	CREATE UNIQUE INDEX "mp_contentstyle_pkey"  ON "mp_contentstyle"("guid");     CREATE INDEX idxcstylename ON mp_contentstyle(name);CREATE INDEX idxcstyleskin ON mp_contentstyle(skinname);ALTER TABLE mp_contentstyleADD CONSTRAINT pk_contentstyle PRIMARY KEY (guid);  INSERT INTO mp_sitesettingsexdef(keyname,defaultvalue,groupname,sortorder)  VALUES('WebSnaprKey','','APIKeys', 100);